package hlc.ud04.autentificadorapp.autenticador;

import hlc.ud04.appsec.seguridad.autenticacion.RespuestaDesafio;

public class RespuestaDesafioPassword implements RespuestaDesafio {
	
	private String respuesta;
	private int numeroCifrador;

	public RespuestaDesafioPassword(String respuesta, int numeroCifrador) {
		this.respuesta = respuesta;
		this.numeroCifrador = numeroCifrador;
		
	}
	
	/**
	 * @return the respuesta
	 */
	public String getRespuesta() {
		return respuesta;
	}
	
	public int getNumeroCifrador() {
		return numeroCifrador;
	}
	

}
